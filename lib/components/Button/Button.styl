@import '../../helpers/stylus.helper.styl';small-height = 24pxsmall-min-width = 50pxdefault-height = 32pxdefault-min-width = 80pxbig-height = 48pxbig-min-width = 110pxprefix = class-prefix + "-btn"common-style(color)  color #fff  border none  background color  &.{prefix}-outline    color color    background #fff    border: 1.2px solid colorrounded-style(height)  &.{prefix}-rounded    border-radius (height/2)px// circle 样式circle-style(height)  &.{prefix}-circle    max-width height    min-width height    border-radius 50%    padding 0.{prefix}  white-space nowrap  border-radius 2px  box-shadow 0 2px 5px 0 rgba(0, 0, 0, .14), 0 2px 10px 0 rgba(0, 0, 0, .1)  display inline-block  text-align center  line-height 1  margin 8px 8px 8px 0  padding 0 15px  font-size 14px  min-width default-min-width  height default-height  overflow hidden  cursor pointer  transition all .2s  font-weight 400  rounded-style(default-height)  circle-style(default-height)  &:hover    box-shadow 0 2px 5px 0 rgba(0, 0, 0, .24), 0 2px 10px 0 rgba(0, 0, 0, .24)  &:focus    outline none  &:active    opacity 0.85  //disabled样式  &[disabled]    cursor not-allowed    box-shadow 0 2px 5px 0 rgba(0, 0, 0, .16), 0 2px 10px 0 rgba(0, 0, 0, .12)    opacity 0.6  &:active    opacity none//颜色样式.{prefix}-primary  common-style(primary-color).{prefix}-success  common-style(success-color).{prefix}-danger  common-style(danger-color).{prefix}-info  common-style(info-color).{prefix}-warning  common-style(warning-color).{prefix}-dark  common-style(dark-color).{prefix}-gray  common-style(gray-color)  color #666 !important  box-shadow none//大小样式.{prefix}-small  height small-height  min-width small-min-width  padding 0 7px  font-size 12px  rounded-style(small-height)  circle-style(small-height).{prefix}-big  height big-height  min-width big-min-width  rounded-style(big-height)  circle-style(big-height)// block 样式.{prefix}-block  width 100%  margin: 4px